{
	"name": "comby-vscode",
	"displayName": "Comby",
	"description": "Extension for comby in vscode",
	"version": "0.0.1",
	"publisher": "giltho",
	"engines": {
		"vscode": "^1.40.0"
	},
	"activationEvents": [
		"onCommand:comby-vscode.searchAndReplace"
	],
	"categories": [
		"Other"
	],
	"repository": {
		"type" : "git",
		"url" : "https://github.com/giltho/comby-vscode.git"
	},
	"keywords": ["comby", "rewrite"],
	"icon": "media/comby.png",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "comby-vscode.searchAndReplace",
				"title": "Comby: Match And Replace"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {}
}
